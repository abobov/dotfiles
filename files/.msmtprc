defaults
    logfile ~/Mail/msmtp.log

account gmail
    host smtp.gmail.com
    from abobov@gmail.com
    auth on
    tls on
    tls_trust_file /etc/ssl/certs/ca-certificates.crt
    tls_starttls on
    # Password stored in ~/.netrc
    user abobov@gmail.com
    port 587

account default: gmail

# vim: nobackup noswapfile :
